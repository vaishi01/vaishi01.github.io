{
  "name": "@aliimam/icons",
  "version": "0.1.7",
  "license": "MIT",
  "description": "Ali Imam React SVG Icon Set",
  "source": "./src/index.tsx",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "generate-src": "generate-icon-lib --file=9LLDXYb3i7M7fg3sGFHkyQ && cd ./icons",
    "build": "rimraf dist && pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist",
    "src",
    "icons",
    "manifest.json"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "module": "dist/index.js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/csj/index.d.ts",
          "default": "./dist/cjs/index.js"
        }
      },
      "./icons/*.svg": "./icons/*.svg",
      "./*": {
        "import": {
          "types": "./dist/*.d.ts",
          "default": "./dist/*.js"
        },
        "require": {
          "types": "./dist/csj/*.d.ts",
          "default": "./dist/cjs/*.js"
        }
      }
    }
  },
  "peerDependencies": {
    "react": "^16.x || ^17.x || ^18.x || ^19.x"
  },
  "devDependencies": {
    "@repo/generate-icon-lib": "workspace:*",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "react": "^19.1.1",
    "tsdx": "^0.14.1",
    "typescript": "^5.9.2"
  }
}
